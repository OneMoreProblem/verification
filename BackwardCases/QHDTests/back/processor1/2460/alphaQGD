FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "2460";
    object      alphaQGD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0.5;
    }
    walls
    {
        type            zeroGradient;
    }
    symwall
    {
        type            empty;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0.1;
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 0.1;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 0.1;
    }
}


